
function checkEnter(myfield,e){
    if (!e){
      var e = window.event;
    } 
    if (e.keyCode){
      code = e.keyCode;    // IE   
    }else if (e.which){
	  code = e.which;      // Other browsers
	}
	if (code == 13){
	    return false;
	}	
}	



function checkTab(myfield,e){
    if (!e){
      var e = window.event;
    } 
    if (e.keyCode){
      code = e.keyCode;    // IE   
    }else if (e.which){
	  code = e.which;      // Other browsers
	}
	myid = myfield.id;

	if (code == 9){
	    if (myid == "case_inpatient"){
			$('#case_patid1').focus();
			return false;
		}else if (myid == "case_emergency"){
			$('#case_anes_sid').focus();
			return false;
		}else if (myid == 'case_otherproc2_desc'){
			$('#casescrolldiv').scrollTop(1790);
			$('#require_addp_check').focus();
			return false;
		}else if (myid == 'case_surgical_amount'){
			$('#case_cpt_surg').focus();
			return false;
		}else if (myid == 'case_postop_block_req'){
			return false;
		}else if (myid == 'case_addproc14_days'){
			$('#casescrolldiv').scrollTop(2225);
		}
		return true;
	}
}	



